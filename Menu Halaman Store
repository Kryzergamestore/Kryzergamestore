<script>

var acc = document.getElementsByClassName("accordion");

var i;



for (i = 0; i < acc.length; i++) {

  acc[i].addEventListener("click", function() {

    this.classList.toggle("active");

    var panel = this.nextElementSibling;

    if (panel.style.maxHeight) {

      panel.style.maxHeight = null;

    } else {

      panel.style.maxHeight = panel.scrollHeight + "px";

    } 

  });

}

</script>



<script>

  const tooltips2 = document.querySelectorAll('.tooltip2');

  tooltips2.forEach(tooltip2 => {

    tooltip2.addEventListener('click', (e) => {

      const closeTooltip = tooltip2.children[1].children[0];

      const toolTipParent = tooltip2.children[1]

      if(e.target === closeTooltip || e.target === closeTooltip.children[0] || e.target === closeTooltip.children[0].children[0] || e.target === closeTooltip.children[0].children[1]) {

        toolTipParent.style.visibility = 'hidden';

        toolTipParent.style.opacity = '0';

      }else {

        toolTipParent.style.visibility = 'inherit';

        toolTipParent.style.opacity = '1';

      }



    })

  })

</script>



